import java.util.*;

public class TwoPointer{
    public static boolean isPalindrome(String s) {

        int start=0;
        int end =s.length()-1;

       while(start<end){

           if(s.charAt(start)!=s.charAt(end))
           {
               return false;
           }
       }
       start++;
       end --;
       
        // Replace this placeholder return statement with your code
        return true;
    }


}
